{% extends 'registration/base.html.twig' %}
 {% block body %}
 {{ include('admin/navigation.html.twig') }}
 <div class="">
    <div class="container">
        <div class="card o-hidden border-0 shadow-lg my-5">
            <div class="card-body p-0">
                <!-- Nested Row within Card Body -->
                <div class="row">
                    <div class="col-lg-7">
                        <div class="p-5">
                            <div class="text-center">
                                <h1 class="h4 text-gray-900 mb-4">Create About Section</h1>
                            </div>
                            {{ form_start(aboutForm) }}
                                <div class="form-group row">
                                    <div class="col-sm-6 mb-3 mb-sm-0">
                                        {{ form_row(aboutForm.email_address) }}  
                                      
                                    </div>
                                    <div class="col-sm-6">
                                         {{ form_row(aboutForm.phone_number) }}
                                    </div>
                                </div>
                                <div class="form-group">
                                   
                                        {{ form_row(aboutForm.fax_number) }}
                                </div>
                                 <div class="form-group">
                                   
                                        {{ form_row(aboutForm.telephone) }}
                                </div>
                                 <div class="form-group">
                                   
                                        {{ form_row(aboutForm.mission_statement) }}
                                </div>
                                 <div class="form-group">
                                   
                                        {{ form_row(aboutForm.vision) }}
                                </div>
                                 <div class="form-group">
                                   
                                        {{ form_row(aboutForm.motto) }}
                                </div>
                                 <div class="form-group">
                                   
                                        {{ form_row(aboutForm.history) }}
                                </div>
                                <a>
                                  <button type="submit" class="btn">{{form_row(aboutForm.submit)}}</button>
                                </a>
                             {{ form_end(aboutForm) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
 {{ include('admin/footer.html.twig') }}
 {% endblock %}