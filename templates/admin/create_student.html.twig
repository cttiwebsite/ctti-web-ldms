{% extends 'registration/base.html.twig' %}
 {% block body %}
 {{ include('admin/navigation.html.twig') }}
 <div class="">
    <div class="container">
        <div class="card o-hidden border-0 shadow-lg my-5">
            <div class="card-body p-0">
                <!-- Nested Row within Card Body -->
                <div class="row">
                    <div class="col-lg-7">
                        <div class="p-5">
                            <div class="text-center">
                                <h1 class="h4 text-gray-900 mb-4">Create Student</h1>
                            </div>
                            {% for message in app.flashes('success') %}
                              <div class="alert alert-success">
                               {{ message }}
                               </div>
                            {% endfor %}
                            {{ form_start(studentForm) }}
                                <div class="form-group row">
                                    <div class="col-sm-6 mb-3 mb-sm-0">
                                          {{ form_row(studentForm.student_nrc) }}  
                                    </div>
                                    <div class="col-sm-6">
                                         {{ form_row(studentForm.student_id) }}
                                    </div>
                                </div>
                                <div class="form-group">  
                                        {{ form_row(studentForm.student_first_name) }}
                                </div>
                                <div class="form-group">  
                                        {{ form_row(studentForm.student_middle_name) }}
                                </div>
                                  <div class="form-group">    
                                        {{ form_row( studentForm.student_last_name)}}
                                </div>
                                 <div class="form-group"> 
                                        {{ form_row(studentForm.student_sponsor) }}
                                </div>
                                 <div class="form-group">
                                        {{ form_row(studentForm.student_email) }}
                                </div>
                                 <div class="form-group">
                                        {{ form_row(studentForm.student_section) }}
                                </div>
                                 <div class="form-group">
                                        {{ form_row(studentForm.student_program_of_study) }}
                                </div>
                                 <div class="form-group">
                                        {{ form_row(studentForm.student_gender) }}
                                </div>
                                 <div class="form-group">
                                        {{ form_row(studentForm.student_date_of_birth) }}
                                </div>
                                 <div class="form-group">
                                   
                                </div>
                                  <div class="form-group">
                                        {{ form_row(studentForm.student_disability) }}
                                </div>
                                <a>
                                  <button type="submit" class="btn">{{form_row(studentForm.submit)}}</button>
                                </a>
                             {{ form_end(studentForm) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
 {{ include('admin/footer.html.twig') }}
 {% endblock %}